{
  "name": "gelf-simple-js-logger",
  "version": "1.0.2",
  "description": "gelf-simple-js-logger is a simple logger that sends logs to Graylog using the GELF (Graylog Extended Log Format) protocol and console. This package allows you to configure the logger to send logs via UDP or TCP.",
  "main": "src/logger.js",
  "scripts": {
    "start": "node --expose-gc src/logger.js"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "logger",
    "gelf",
    "graylog",
    "logs",
    "udp",
    "tcp"
  ],
  "author": "Marcelo Brake",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelobrake/gelf-simple-js-logger.git"
  },
  "bugs": {
    "url": "https://github.com/marcelobrake/gelf-simple-js-logger/issues"
  },
  "homepage": "https://github.com/marcelobrake/gelf-simple-js-logger#readme"
}
